{
  "name": "alto-explorer",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.126",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:wasm": "cd ../types && wasm-pack build --target web --out-dir ../explorer/src/alto_types --out-name alto_types",
    "build": "npm run build:wasm && react-scripts build",
    "build:react": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "es2020": true
    },
    "ignorePatterns": [
      "src/alto_types/**/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.16"
  }
}